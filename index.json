            "custom_liquid_xT89EF": {
              "type": "custom-liquid",
              "name": "t:names.custom_liquid",
              "settings": {
                "custom_liquid": "<div class=\"contenedor-rotacion\">\n  <img \n    src=\"https://cdn.shopify.com/s/files/1/0773/2776/1630/files/Copia-de-Brown-Minimalist-Aesthetic-Moodboard-Photo-Collage-Instagram-Post-4.webp?v=1766606346\" \n    alt=\"Blue Zone\" \n    class=\"imagen-gira-scroll\"\n  >\n</div>\n\n<style>\n  .contenedor-rotacion {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    height: 100%;\n    overflow: hidden;\n    padding: 20px; /* Agregué un poco de espacio para que no se corte al girar */\n  }\n\n  .imagen-gira-scroll {\n    display: block;\n    width: 100%;\n    max-width: 600px; /* Opcional: Limita el tamaño para que no sea gigante */\n    height: auto;\n    transition: transform 0.1s ease-out; \n    will-change: transform;\n  }\n</style>\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function() {\n    const imagen = document.querySelector(\".imagen-gira-scroll\");\n    \n    if (imagen) {\n      window.addEventListener(\"scroll\", function() {\n        const scrollPosition = window.scrollY;\n        \n        // CAMBIO AQUÍ:\n        // Antes era / 5. Ahora es / 20.\n        // Esto hace que la rotación sea 4 veces más pequeña/lenta.\n        const rotation = scrollPosition / 20;\n        \n        imagen.style.transform = `rotate(${rotation}deg)`;\n      });\n    }\n  });\n</script>"
              },
              "blocks": {}
            }
